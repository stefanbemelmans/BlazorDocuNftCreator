@inherits FormConfirmationComponentModel
@if (FormData != null)
{
    <h4>Confirm Data</h4>
  <div class="row m-2 p-2">

    <h3>@FormData.Title</h3>
    <h4>@MutableDataString</h4>
    @foreach (var prop in FormData.GetType().GetProperties())
    {
      <div class="col-6 col-sm-5 col-lg-3 col-xl-2 text-center m-2 bg-success">
        <h4>@prop.Name:</h4>
        <h5>@prop.GetValue(FormData)</h5>
      </div>
    }

 
  </div>
}