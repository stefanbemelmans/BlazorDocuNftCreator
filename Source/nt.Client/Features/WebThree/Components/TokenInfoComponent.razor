@inherits TokenInfoModel

<div class="flex-column justify-content-center m-2 p-2">
  <h3 class="text-center">Token Template Info</h3>
  <div class="container bg-danger">
    <div class="row">
      <div class="col">
        <h4>Name: @TokenData.Name</h4>
      </div>
      <div class="col">
        <h4>Symbol: @TokenData.Symbol</h4>
      </div>
      <div class="col">
        <h4>MintLimit: @TokenData.MintLimit</h4>
      </div>
      <div class="col">
        <h4>AttachedTokens: @TokenData.AttachedTokens</h4>
      </div>
    </div>
  </div>
  <div class="container bg-info mt-4">
    <div class="row">
      @foreach (var property in TokenData.NftProps)
      {
        <div class="col text-center">
          <h4>@property.Key</h4>
          <h5 class="bg-light">@property.Value</h5>
        </div>
      }
    </div>
  </div>
</div>
